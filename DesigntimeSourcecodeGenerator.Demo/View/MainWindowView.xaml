<UserControl x:Class="DesigntimeSourcecodeGenerator.Demo.View.MainWindowView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DesigntimeSourcecodeGenerator.Demo.View"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <Grid>
        <Button Width="100"
                Height="100"
                Margin="10"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Command="{Binding ClickCommand}">
            Click!
        </Button>
        <Button Width="100"
                Height="100"
                Margin="10"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Command="{Binding Click2Command}">
            Click2!
        </Button>
        <Button Width="100"
                Height="100"
                Margin="10"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Command="{Binding Click3Command}"
                CommandParameter="123456789">
            Click3!
        </Button>
    </Grid>
</UserControl>
